package Lab3;
import java.util.*;
import java.io.File;
import java.io.FileNotFoundException;

public class Lab3 {

	public static void main(String[] args) {
		String key1 = "";
		String key2 = "";
		Scanner in = null;
		try {
		    in = new Scanner(new File("dates.txt"));
		} catch (FileNotFoundException exception) {
		    System.err.println("failed to open dates.txt");
		    System.exit(1);
		}
		while (in.hasNextLine()) {
		    String line = in.nextLine();
		    Date date = new Date(line);
		    System.out.println(date);
		    // need more code for DateRange objects
		    if (key1.equals(""))
		    	key1 = line;
		    if (!key1.equals("") && key2.equals(""))
		    	key2 = line;
		    //if ((!key1.equals("") && !key2.equals("")) && date.getDate())
		}

	}

}
